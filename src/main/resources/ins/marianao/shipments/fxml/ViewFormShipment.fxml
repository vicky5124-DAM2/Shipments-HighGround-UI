         <children>
            <TextField fx:id="width" layoutX="325.0" prefHeight="24.0" prefWidth="120.0" promptText="%fxml.text.viewFormShipment.width"/>
            <TextField prefHeight="24.0" layoutX="525.0" prefWidth="120.0" promptText="%fxml.text.viewFormShipment.height"/>
            <TextField prefHeight="24.0" layoutX="725.0" prefWidth="120.0" promptText="%fxml.text.viewFormShipment.depth"/>
         </children></HBox>
      <TextField fx:id="priority" layoutX="325.0" layoutY="480.0" prefHeight="24.0" prefWidth="500.0" promptText="%fxml.text.viewFormShipment.priority" />
      <Button fx:id="bCreate" layoutX="325.0" layoutY="530.0" mnemonicParsing="false" prefHeight="33.0" prefWidth="500.0" text="%fxml.text.viewFormShipment.create" />
   </children>
</AnchorPane>
